# Choose a prime p
p = 3
# choose n
n = 2
# Set the sum S = 0
S <- 0
for (k in 1:n){
S <- S + k*p^(-(n-k)^2) * P
# Reset the product P = 1
P <- 1
for (j in 0:(k-1)){
P <- P * ((1-p^(j-n))^2)/(1-p^(j-k))
}
}
